ruleset ShowLocation {
  meta {  
//b505289x5.prod
    name "Show Location"
    description <<
      Checkin In With Foursquare
    >>
    author ""
    logging off
	use module a169x701 alias CloudRain
	use module a41x186  alias SquareTag
	
	use module b505289x4 alias LocationData

  }
  
  dispatch {
	}


  rule show_fs_location is active {
		select when pageview ".*" {
		checkin = location_data:get_location_data("fs_checkin")
	  	notify("Hello World", "I can make a Notify") with sticky = true;
		
	}
	}
